# Test .env file to demonstrate dataSource attributes functionality

# SentinelOne API Configuration
AISIEM_LOGACCESS_WRITE_TOKEN=test-token-here
AISIEM_SERVER=xdr.us1.sentinelone.net
SYSLOG_HOST=TEST_HOST

# Port Configuration
PORT1_PROTOCOL=udp
PORT1_NUMBER=514
PORT1_TYPE=syslog
PORT2_PROTOCOL=tls
PORT2_NUMBER=6514
PORT2_TYPE=syslog

# Source Type 1 Configuration - WITH dataSource attributes
SOURCE1_NAME=cisco-router
SOURCE1_PARSER=ciscoRouter
SOURCE1_ATTRIBUTE=hostname
SOURCE1_MATCHER=router*
# Optional: DataSource attributes (leave blank to skip)
SOURCE1_DATASOURCE_NAME="Cisco Router"
SOURCE1_DATASOURCE_VENDOR="Cisco"

# Source Type 2 Configuration - WITHOUT dataSource attributes (blank)
SOURCE2_NAME=cisco-firewall
SOURCE2_PARSER=ciscoFirewall
SOURCE2_ATTRIBUTE=appname
SOURCE2_MATCHER=firewall*
# Optional: DataSource attributes (leave blank to skip)
SOURCE2_DATASOURCE_NAME=""
SOURCE2_DATASOURCE_VENDOR=""

# Legacy API_TOKEN (for backward compatibility)
API_TOKEN=${AISIEM_LOGACCESS_WRITE_TOKEN}
